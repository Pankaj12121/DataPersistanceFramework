<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="tests" thread-count="5">

	<parameter name="browsername" value="chrome" />
	<parameter name="browserversion" value="6" />
	<parameter name="platform" value="windows" />

	<listeners>
		<listener class-name="com.demo.excelReport.CustomReporter"></listener>
	</listeners>
	<test verbose="2" name="Chrome" parallel="methods" thread-count="5">
		<packages>
			<package name="com.demo.tests.*" />
		</packages>

		<method-selectors>
			<method-selector>
				<script language="beanshell"><![CDATA[
         			groups.containsKey("functional")
       			]]></script>
			</method-selector>
		</method-selectors>
	</test>
</suite>
